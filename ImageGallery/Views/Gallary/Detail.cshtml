@model ImageGallery.Models.GallaryDetailModel
@{
    ViewData["Title"] = "Detail";
}


<br />
<div class="container body-content">
    <div class="row image-detail-container">
        <img src="@Model.Url"alt="@Model.Title" />
    </div>
    <div class="row image-detail-meta">
        <span> Title: @Model.Title</span>
        <span> Upload : @Model.CreateOn</span>
    </div>
    <div class="row controls">
        <a class="btn btn-primary" asp-controller="Gallary" asp-action="Index">
            Back to Gallery
        </a>
    </div>
</div>

